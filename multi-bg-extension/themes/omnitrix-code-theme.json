  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#4d8866",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#ffcc00"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#ff00ff"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#ff66ff"
      }
    },
    {
      "scope": ["constant.character.escape", "constant.other.placeholder"],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#00ff41",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.control", "keyword.operator"],
      "settings": {
        "foreground": "#4dff7f"
      }
    },
    {
      "scope": ["keyword.operator.logical", "keyword.operator.comparison", "keyword.operator.assignment"],
      "settings": {
        "foreground": "#00ff41"
      }
    },
    {
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#4da6ff",
        "fontStyle": ""
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#00ffff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#00ff41",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#66ff99",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#e0f2e9"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#ffcc00",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#ff66ff",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#ff66ff"
      }
    },
    {
      "scope": ["meta.import", "meta.export"],
      "settings": {
        "foreground": "#00ff41"
      }
    },
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#00ff41"
      }
    },
    {
      "scope": ["punctuation.separator", "punctuation.terminator"],
      "settings": {
        "foreground": "#a8d4bb"
      }
    },
    {
      "scope": ["meta.brace", "punctuation.section"],
      "settings": {
        "foreground": "#00ff41"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#00ff41",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#ffcc00",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#00ff41",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#4da6ff",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#66ff99",
        "background": "#0f2520"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#4d8866",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#00ff41"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ffffff",
        "background": "#ff3333"
      }
    },
    {
      "scope": ["entity.name.section"],
      "settings": {
        "foreground": "#00ffff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.property-name", "support.type.property-name"],
      "settings": {
        "foreground": "#66ff99"
      }
    },
    {
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#66ff99"
      }
    },
    {
      "scope": ["support.variable"],
      "settings": {
        "foreground": "#ff66ff"
      }
    },
    {
      "scope": ["entity.name.module"],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "scope": ["variable.other.constant"],
      "settings": {
        "foreground": "#ff66ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.decorator"],
      "settings": {
        "foreground": "#ff00ff"
      }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#00ffff"
      }
    },
    {
      "scope": ["support.other.namespace"],
      "settings": {
        "foreground": "#00ffff"
      }
    }
  ]
}