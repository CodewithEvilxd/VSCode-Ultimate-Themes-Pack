  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#ca7113",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings & Template Literals",
      "scope": [
        "string",
        "string.template",
        "constant.character.escape",
        "string.regexp",
        "constant.other.symbol",
        "support.constant"
      ],
      "settings": {
        "foreground": "#b44a15"
      }
    },
    {
      "name": "Keywords & Control",
      "scope": [
        "keyword",
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#574535"
      }
    },
    {
      "name": "Functions & Methods",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#ca7113"
      }
    },
    {
      "name": "Variables & Properties",
      "scope": [
        "variable",
        "entity.name.variable",
        "meta.property-name",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#4A5141"
      }
    },
    {
      "name": "Types & Interfaces",
      "scope": [
        "entity.name.type",
        "support.type",
        "entity.name.class",
        "storage.type.class"
      ],
      "settings": {
        "foreground": "#b44a15"
      }
    },
    {
      "name": "Constants & Enums",
      "scope": [
        "constant",
        "constant.numeric",
        "support.constant"
      ],
      "settings": {
        "foreground": "#ca7113"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "support.constant.numeric"
      ],
      "settings": {
        "foreground": "#ca7113"
      }
    },
    {
      "name": "Operators & Punctuation",
      "scope": [
        "keyword.operator",
        "punctuation.definition.tag",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#7A5F8B"
      }
    },
    {
      "name": "Decorators & Annotations",
      "scope": [
        "meta.decorator",
        "storage.modifier.annotation"
      ],
      "settings": {
        "foreground": "#5F8C78"
      }
    },
    {
      "name": "Data Keys (JSON, objects)",
      "scope": [
        "support.type.property-name.json",
        "meta.object-literal.key",
        "meta.mapping.key.json",
        "meta.structure.dictionary.json string.quoted.double.json",
        "support.type.property-name",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#2F291F"
      }
    },
        {
      "name": "Data Keys (JSON, objects)",
      "scope": [
        "meta.structure.dictionary.json string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#ca7113"
      }
    },
    {
      "name": "HTML & JSX Tags",
      "scope": [
        "entity.name.tag",
        "support.class.component",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#5A4B39"
      }
    },
    {
      "name": "HTML Attribute Names",
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.attribute-name",
        "support.variable.attribute"
      ],
      "settings": {
        "foreground": "#6B9583"
      }
    },
    {
      "name": "CSS Property Names",
      "scope": [
        "support.type.property-name.css",
        "meta.property-list.css meta.property-name",
        "entity.other.attribute-name.css"
      ],
      "settings": {
        "foreground": "#7A6754"
      }
    }
  ]
}