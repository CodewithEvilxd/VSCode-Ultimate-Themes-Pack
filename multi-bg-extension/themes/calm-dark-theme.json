  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#7f9916",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings & Template Literals",
      "scope": [
        "string",
        "string.template",
        "constant.character.escape",
        "string.regexp",
        "constant.other.symbol",
        "support.constant"
      ],
      "settings": {
        "foreground": "#dbd3ce"
      }
    },
    {
      "name": "Keywords & Control",
      "scope": [
        "keyword",
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#dedbd3"
      }
    },
    {
      "name": "Functions & Methods",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#7db19c"
      }
    },
    {
      "name": "Variables & Properties",
      "scope": [
        "variable",
        "entity.name.variable",
        "meta.property-name",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#6499b5"
      }
    },
    {
      "name": "Types & Interfaces",
      "scope": [
        "entity.name.type",
        "support.type",
        "entity.name.class",
        "storage.type.class"
      ],
      "settings": {
        "foreground": "#E09B61"
      }
    },
    {
      "name": "Constants & Enums",
      "scope": [
        "constant",
        "constant.numeric",
        "support.constant"
      ],
      "settings": {
        "foreground": "#BA7363"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "support.constant.numeric"
      ],
      "settings": {
        "foreground": "#C1834F"
      }
    },
    {
      "name": "Operators & Punctuation",
      "scope": [
        "keyword.operator",
        "punctuation.definition.tag",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#B3A799"
      }
    },
    {
      "name": "Decorators & Annotations",
      "scope": [
        "meta.decorator",
        "storage.modifier.annotation"
      ],
      "settings": {
        "foreground": "#87BFA8"
      }
    },
    {
      "name": "Data Keys (JSON, objects)",
      "scope": [
        "support.type.property-name.json",
        "meta.object-literal.key",
        "meta.mapping.key.json",
        "support.type.property-name",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#C1B8AA"
      }
    },
    {
      "name": "Data Keys (JSON, objects)",
      "scope": [
        "meta.structure.dictionary.json string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#E09B61"
      }
    },
    {
      "name": "HTML & JSX Tags",
      "scope": [
        "entity.name.tag",
        "support.class.component",
        "meta.tag"
      ],
      "settings": {
        "foreground": "#CBBCA4"
      }
    },
    {
      "name": "HTML Attribute Names",
      "scope": [
        "entity.other.attribute-name",
        "meta.tag.attribute-name",
        "support.variable.attribute"
      ],
      "settings": {
        "foreground": "#8FCAB7"
      }
    },
    {
      "name": "CSS Property Names",
      "scope": [
        "support.type.property-name.css",
        "meta.property-list.css meta.property-name",
        "entity.other.attribute-name.css"
      ],
      "settings": {
        "foreground": "#B49B82"
      }
    }
  ]
}