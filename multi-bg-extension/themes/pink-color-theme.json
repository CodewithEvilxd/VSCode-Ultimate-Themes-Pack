{
  "name": "Blush Pink Muted",
  "type": "light",
  "colors": {
    "editor.background": "#f0c9d2", 
    "editor.foreground": "#2a2a2a",
    "editorLineNumber.foreground": "#8a5c70",
    "editorCursor.foreground": "#b03a58",
    "editor.selectionBackground": "#e9aabf",
    "editor.lineHighlightBackground": "#e7b7c5",
    "editorIndentGuide.background1": "#c07a8f",
    "editorWidget.background": "#eac3cf",
    "editorWidget.border": "#d192a5",
    "list.activeSelectionBackground": "#e9aabf",
    "list.inactiveSelectionBackground": "#eac3cf",
    "sideBar.background": "#e5c0cb",
    "sideBar.foreground": "#2a2a2a",
    "activityBar.background": "#d78ba2",
    "activityBar.foreground": "#2a2a2a",
    "panel.background": "#e5c0cb",
    "statusBar.background": "#d78ba2",
    "statusBar.foreground": "#2a2a2a",
    "titleBar.activeBackground": "#e9aabf",
    "titleBar.activeForeground": "#2a2a2a",
    "input.background": "#eac3cf",
    "input.foreground": "#2a2a2a",
    "dropdown.background": "#eac3cf",
    "dropdown.foreground": "#2a2a2a"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment"],
      "settings": {
        "foreground": "#7a5a65",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#b03a58"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#922d4a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#503080"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#a23e60"
      }
    }
  ]
}
