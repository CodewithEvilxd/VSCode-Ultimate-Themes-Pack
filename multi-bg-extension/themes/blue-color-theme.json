      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#ffd166"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#e0eaff"
      }
    }
  ]
