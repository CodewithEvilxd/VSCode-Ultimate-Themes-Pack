  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6A8A78",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#D4A84B"
      }
    },
    {
      "name": "String Escapes",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#E8C46B"
      }
    },
    {
      "name": "Template Strings",
      "scope": ["string.template", "punctuation.definition.string.template"],
      "settings": {
        "foreground": "#D4A84B"
      }
    },
    {
      "name": "Template Expression",
      "scope": ["punctuation.definition.template-expression"],
      "settings": {
        "foreground": "#B88FB8"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#E08A8A"
      }
    },
    {
      "name": "Built-in Constants",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#E08A8A",
        "fontStyle": "italic"
      }
    },
    {
      "name": "User Constants",
      "scope": ["constant.other", "variable.other.constant"],
      "settings": {
        "foreground": "#D4A84B"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "keyword.control"],
      "settings": {
        "foreground": "#B88FB8"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#6BC4B8"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage", "storage.type"],
      "settings": {
        "foreground": "#B88FB8"
      }
    },
    {
      "name": "Storage Modifier",
      "scope": ["storage.modifier"],
      "settings": {
        "foreground": "#B88FB8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#8FB89F"
      }
    },
    {
      "name": "Function Parameters",
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#E8C46B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.class",
        "entity.name.type.class",
        "support.class"
      ],
      "settings": {
        "foreground": "#6B9AC4"
      }
    },
    {
      "name": "Inherited Classes",
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#8AB8E0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Types",
      "scope": [
        "entity.name.type",
        "support.type",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#6BC4B8"
      }
    },
    {
      "name": "Interfaces",
      "scope": ["entity.name.type.interface"],
      "settings": {
        "foreground": "#6BC4B8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#E8DFD0"
      }
    },
    {
      "name": "Object Properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#A8D4B8"
      }
    },
    {
      "name": "This/Self",
      "scope": ["variable.language.this", "variable.language.self"],
      "settings": {
        "foreground": "#E08A8A",
        "fontStyle": "italic"
      }
    },
    {
      "name": "HTML Tags",
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#C96B6B"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#D4A84B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSX Component Tags",
      "scope": [
        "support.class.component",
        "entity.name.tag.js",
        "entity.name.tag support.class.component"
      ],
      "settings": {
        "foreground": "#6B9AC4"
      }
    },
    {
      "name": "CSS Selectors",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#D4A84B"
      }
    },
    {
      "name": "CSS Properties",
      "scope": ["support.type.property-name.css"],
      "settings": {
        "foreground": "#8FB89F"
      }
    },
    {
      "name": "CSS Values",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#E8C46B"
      }
    },
    {
      "name": "CSS Units",
      "scope": ["keyword.other.unit.css"],
      "settings": {
        "foreground": "#E08A8A"
      }
    },
    {
      "name": "CSS Pseudo",
      "scope": ["entity.other.attribute-name.pseudo-class.css"],
      "settings": {
        "foreground": "#B88FB8"
      }
    },
    {
      "name": "JSON Keys",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#8FB89F"
      }
    },
    {
      "name": "JSON Values",
      "scope": ["string.quoted.double.json"],
      "settings": {
        "foreground": "#D4A84B"
      }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "heading.1.markdown",
        "heading.2.markdown",
        "heading.3.markdown",
        "heading.4.markdown",
        "heading.5.markdown",
        "heading.6.markdown",
        "markup.heading"
      ],
      "settings": {
        "foreground": "#6B9AC4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#E8C46B",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown Italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#B88FB8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown Links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#6BC4B8"
      }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#8FB89F"
      }
    },
    {
      "name": "Markdown Quote",
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#6A8A78",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#A8C4B0"
      }
    },
    {
      "name": "Brackets",
      "scope": [
        "punctuation.definition.block",
        "punctuation.definition.parameters",
        "punctuation.definition.arguments"
      ],
      "settings": {
        "foreground": "#A8C4B0"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#6BC4B8"
      }
    },
    {
      "name": "Decorators/Annotations",
      "scope": [
        "meta.decorator",
        "meta.annotation"
      ],
      "settings": {
        "foreground": "#D4A84B",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Import/Export",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from"
      ],
      "settings": {
        "foreground": "#B88FB8"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#C96B6B",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#D49150",
        "fontStyle": "strikethrough"
      }
    }
  ]
}