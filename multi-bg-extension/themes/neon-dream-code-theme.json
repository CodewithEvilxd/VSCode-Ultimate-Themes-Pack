  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5865a8",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#05ffa1"
      }
    },
    {
      "scope": ["string.template"],
      "settings": {
        "foreground": "#05ffa1"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#ff2a6d"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#d896ff"
      }
    },
    {
      "scope": ["constant.character.escape", "constant.other.placeholder"],
      "settings": {
        "foreground": "#00f0ff"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#ff2a6d",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#ff2a6d",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#00f0ff"
      }
    },
    {
      "scope": ["keyword.operator.logical", "keyword.operator.comparison", "keyword.operator.assignment"],
      "settings": {
        "foreground": "#00f0ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#fffb00",
        "fontStyle": ""
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#5c9fff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#ff2a6d",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#d896ff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#e4f0fb"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#ffa057",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#bd5eff",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": ["variable.other.constant"],
      "settings": {
        "foreground": "#d896ff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#d896ff"
      }
    },
    {
      "scope": ["meta.import", "meta.export"],
      "settings": {
        "foreground": "#ff2a6d"
      }
    },
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#ff2a6d"
      }
    },
    {
      "scope": ["punctuation.separator", "punctuation.terminator"],
      "settings": {
        "foreground": "#b8c5d6"
      }
    },
    {
      "scope": ["meta.brace", "punctuation.section"],
      "settings": {
        "foreground": "#00f0ff"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#ff2a6d",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#05ffa1",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#fffb00",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#5c9fff",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#05ffa1",
        "background": "#121838"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#5865a8",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#ff2a6d"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ffffff",
        "background": "#ff2a6d"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#ffffff",
        "background": "#bd5eff"
      }
    },
    {
      "scope": ["entity.name.section"],
      "settings": {
        "foreground": "#5c9fff",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.property-name", "support.type.property-name"],
      "settings": {
        "foreground": "#00f0ff"
      }
    },
    {
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#00f0ff"
      }
    },
    {
      "scope": ["support.variable"],
      "settings": {
        "foreground": "#d896ff"
      }
    },
    {
      "scope": ["entity.name.module"],
      "settings": {
        "foreground": "#5c9fff"
      }
    },
    {
      "scope": ["meta.decorator", "meta.annotation"],
      "settings": {
        "foreground": "#bd5eff"
      }
    },
    {
      "scope": ["entity.other.inherited-class"],
      "settings": {
        "foreground": "#5c9fff",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["support.other.namespace"],
      "settings": {
        "foreground": "#5c9fff"
      }
    },
    {
      "scope": ["meta.method.declaration", "meta.method"],
      "settings": {
        "foreground": "#fffb00"
      }
    },
    {
      "scope": ["support.type.primitive"],
      "settings": {
        "foreground": "#d896ff"
      }
    },
    {
      "scope": ["entity.name.namespace"],
      "settings": {
        "foreground": "#5c9fff"
      }
    }
  ]
}