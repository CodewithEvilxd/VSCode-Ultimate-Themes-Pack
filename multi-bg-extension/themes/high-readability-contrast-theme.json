{
  "name": "High Readability Contrast",
  "type": "dark",
  "colors": {
    "editor.background": "#1E1E1E",
    "editor.foreground": "#F5F5F5",
    "editorLineNumber.foreground": "#8E8E8E",
    "editorCursor.foreground": "#FFD166",
    "editor.selectionBackground": "#264653",
    "editor.lineHighlightBackground": "#22272A",
    "editorIndentGuide.background1": "#3A3A3A",
    "editorWidget.background": "#252525",
    "editorWidget.border": "#3A3A3A",
    "sideBar.background": "#1B1B1B",
    "sideBar.foreground": "#F5F5F5",
    "activityBar.background": "#1B1B1B",
    "activityBar.foreground": "#FFD166",
    "panel.background": "#1B1B1B",
    "statusBar.background": "#1B1B1B",
    "statusBar.foreground": "#F5F5F5",
    "titleBar.activeBackground": "#1B1B1B",
    "titleBar.activeForeground": "#F5F5F5",
    "input.background": "#222222",
    "input.foreground": "#F5F5F5"
  },
  "tokenColors": [
    { "scope": ["comment"], "settings": { "foreground": "#8E8E8E", "fontStyle": "italic" } },
    { "scope": ["string"], "settings": { "foreground": "#A6E3A1" } },
    { "scope": ["keyword"], "settings": { "foreground": "#FFB86B", "fontStyle": "bold" } },
    { "scope": ["entity.name.function"], "settings": { "foreground": "#89DDFF" } },
    { "scope": ["constant.numeric"], "settings": { "foreground": "#FFD166" } },
    { "scope": ["variable"], "settings": { "foreground": "#F5F5F5" } }
  ]
}