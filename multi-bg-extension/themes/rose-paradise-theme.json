  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#b88899",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#c2185b"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#d81b60"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#7b1fa2"
      }
    },
    {
      "scope": ["constant.character.escape", "constant.other.placeholder"],
      "settings": {
        "foreground": "#e91e63"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#ad1457",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.control", "keyword.operator"],
      "settings": {
        "foreground": "#c2185b"
      }
    },
    {
      "scope": ["keyword.operator.logical", "keyword.operator.comparison", "keyword.operator.assignment"],
      "settings": {
        "foreground": "#d81b60"
      }
    },
    {
      "scope": ["entity.name.function", "meta.function-call", "support.function"],
      "settings": {
        "foreground": "#00897b",
        "fontStyle": ""
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type", "support.class"],
      "settings": {
        "foreground": "#1976d2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#c2185b",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#00897b",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#37474f"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#5e35b1",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#d81b60",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": ["support.constant"],
      "settings": {
        "foreground": "#7b1fa2"
      }
    },
    {
      "scope": ["meta.import", "meta.export"],
      "settings": {
        "foreground": "#ad1457"
      }
    },
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#c2185b"
      }
    },
    {
      "scope": ["punctuation.separator", "punctuation.terminator"],
      "settings": {
        "foreground": "#6d4c5a"
      }
    },
    {
      "scope": ["meta.brace", "punctuation.section"],
      "settings": {
        "foreground": "#4a2a3d"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#c2185b",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#7b1fa2",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#ad1457",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#1976d2",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#00897b",
        "background": "#ffe8ed"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#b88899",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#c2185b"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ffffff",
        "background": "#e91e63"
      }
    },
    {
      "scope": ["entity.name.section"],
      "settings": {
        "foreground": "#1976d2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.property-name", "support.type.property-name"],
      "settings": {
        "foreground": "#00897b"
      }
    },
    {
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#00897b"
      }
    },
    {
      "scope": ["support.variable"],
      "settings": {
        "foreground": "#7b1fa2"
      }
    },
    {
      "scope": ["entity.name.module"],
      "settings": {
        "foreground": "#1976d2"
      }
    },
    {
      "scope": ["variable.other.constant"],
      "settings": {
        "foreground": "#7b1fa2",
        "fontStyle": "bold"
      }
    }
  ]
}